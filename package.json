{
  "name": "web3-scan-batch-bot",
  "version": "0.0.1",
  "description": "Web3 Scan batch jobs (YouTube summary, Gemini image generation)",
  "private": true,
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "prebuild:new": "rimraf dist-new",
    "build:new": "tsc -p tsconfig.json --outDir dist-new && tsc-alias -p tsconfig.json --dir dist-new",
    "deploy": "node scripts/deploy.js",
    "start": "yarn build && pm2 start ecosystem.config.js",
    "test": "jest --config jest.config.js",
    "lint": "eslint --quiet \"**/*.ts\""
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@google/genai": "^1.34.0",
    "@google-cloud/storage": "^7.0.0",
    "axios": "^0.21.1",
    "date-fns": "^3.5.0",
    "dotenv": "^16.0.0",
    "firebase-admin": "^12.0.0",
    "lodash": "^4.17.21",
    "openai": "^4.0.0",
    "rimraf": "3.0.2",
    "typeorm": "^0.3.20",
    "typeorm-encrypted-column": "^0.0.4",
    "uuid": "^9.0.0",
    "youtube-caption-extractor": "^1.9.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.8",
    "@types/node": "16.11.62",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.5.0",
    "jest": "29.7.0",
    "prettier": "2.7.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  }
}
